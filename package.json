    "svgo": "0.6.6"
  },
  "devDependencies": {
    "broccoli": "^1.1.4",
    "broccoli-fixture": "^1.0.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^7.0.1",
    "mocha": "^5.1.1"
